  "scripts": {
    "build:app": "vite build",
    "dev:app": "vite dev",
    "preview:app": "vite preview",
    "serve": "node server/app.js",
    "dev:server": "nodemon --ext .ts,.js --exec 'ts-node server/app.ts '",
    "build:server": "node esbuild.js",
    "build:preview": "npm run build:app && RUNTIME=node npm run build:server",
    "preview": "node dist/index.js",
    "dev:set-env": "node scripts/set-env.mjs",
    "watch": "nodemon --ext .tsx,.ts,.jsx --watch app --watch server --exec 'npm run build'",
    "dev": "concurrently \"npm run watch\" \"npm run start\"",
    "build": "npm run build:app && npm run build:server",
    "start": "docker run --rm --name wapo -p 8000:80 -v $(pwd)/dist:/home/wapo phalanetwork/wapo-devserver",
    "test": "vitest --testTimeout=60000",
    "ci": "npm run test && npm run build"
  },
